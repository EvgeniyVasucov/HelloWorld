@model IEnumerable<LessonASPNET.ViewModels.EmployeeView>

<!DOCTYPE html>
<html>
<head>
    <title>Evgen.COM</title>
    <meta charset="utf-8" />
</head>
<body>
    <table>
        <tr>
            <th>Id</th>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Отчество</th>
            <th>Возраст</th>
        </tr>
        @foreach (var employee in Model)
        {
        <tr>
            <td>@employee.Id</td>
            <td>@employee.FirstName</td>
            <td>@employee.LastName</td>
            <td>@employee.Patronymic</td>
            <td>@employee.Age</td>
        </tr> 
        }
    </table>
</body>
</html>